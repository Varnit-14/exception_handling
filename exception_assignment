def try_except():
    a=5 
    b=0
    try:
        d=a/b 
    except Exception as e:
        print("YOu cannot divide by zero")
try_except()
print()

#2
subjects=["Americans","Indians"]
verbs=["play","watch"]
objects=["Baseball","Cricket"]
s=""
for i in subjects:
    for j in verbs:
        for k in objects:
            s=i+" "+j+" "+k
            print(s)
